<%- include("includes/head.ejs") %>
<body>
    <h1>This is your username @<%= data.name %></h1>
    <button style="background-color: aqua;">Following</button>
    <button style="background-color: aqua;">Followers</button>
    <h2>Followers:<%= data.followers %> &nbsp; &nbsp; Following:<%= data.following %> </h2>
    <hr>
    <% for(let post of data.posts){ %>
        <img src="<%= post.image%>" alt="catsImage">
        <h2>Likes: <%= post.likes %> &nbsp; &nbsp; Comments: <%= post.comments %></h2>
        <% } %>
        <%- include("includes/footer.ejs") %>
</body>
</html>